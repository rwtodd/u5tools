#!/bin/sh

PROG="@cmddir@/$1"

if [ -f "$PROG" -a -x "$PROG" ]
then
  shift
  exec $PROG $@
else
  if [ "$1" = "" ]
  then
    echo "No subcommand given!"
  else
    echo "Can't find sucommand $1!"
  fi
  echo ""
  echo "Available commands:"
  ls -1 @cmddir@
  echo ""
  echo "Run: info u5tools to learn about available commands."
  exit 1
fi
